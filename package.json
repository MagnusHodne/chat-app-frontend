{
  "name": "chat-app-frontend",
  "version": "1.0.0",
  "description": "Frontend for chat app portfolio project",
  "keywords": [
    "react",
    "tailwind",
    "auth0"
  ],
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MagnusHodne/chat-app-frontend.git"
  },
  "bugs": {
    "url": "https://github.com/MagnusHodne/chat-app-frontend/issues"
  },
  "homepage": "https://github.com/MagnusHodne/chat-app-frontend#readme",
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "dev": "parcel -p 3000 src/index.html",
    "build": "npm install --include=dev && parcel build src/index.html"
  },
  "devDependencies": {
    "@babel/preset-env": "7.16.11",
    "@babel/preset-react": "^7.16.7",
    "buffer": "^6.0.3",
    "jest": "^27.5.1",
    "jest-babel": "^1.0.1",
    "parcel": "^2.7.0",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "process": "^0.11.10",
    "tailwindcss": "^3.1.8"
  },
  "dependencies": {
    "@auth0/auth0-react": "^1.11.0",
    "@fortawesome/fontawesome-free": "^6.1.1",
    "@parcel/transformer-react-refresh-wrap": "^2.7.0",
    "@types/auth0": "^2.35.6",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.59",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.0",
    "typescript": "^4.8.4"
  },
  "babel": {
    "presets": [
      "@babel/preset-react",
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "jest": {
    "testEnvironment": "jsdom",
    "collectCoverageFrom": [
      "**/*.jsx",
      "**/*.js",
      "!dist/**",
      "!index.jsx",
      "!coverage/**"
    ]
  },
  "postcss": {
    "plugins": {
      "tailwindcss": {
        "config": "./tailwind.config.ts"
      }
    }
  }
}
